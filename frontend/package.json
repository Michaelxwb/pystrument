  "devDependencies": {
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.9.4",
    "@types/nprogress": "^0.2.3",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "@vitejs/plugin-vue": "^4.5.0",
    "@vue/eslint-config-prettier": "^8.0.0",
