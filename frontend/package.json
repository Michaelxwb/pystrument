    "axios": "^1.6.2",
    "dayjs": "^1.11.10",
    "echarts": "^5.6.0",
    "element-plus": "^2.4.4",
    "html2pdf.js": "^0.11.2",
    "lodash-es": "^4.17.21",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.7",
    "vue": "^3.3.8",
    "vue-router": "^4.2.5"
